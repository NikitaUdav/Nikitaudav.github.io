 calcRem(13px);\r\n\r\n$max-width: 1200px;\r\n\r\n$screen-iphone: 375px;\r\n$screen-iphone-plus: 414px;\r\n$screen-tablet: 768px;\r\n$screen-desktop: 1024px;\r\n$screen-desktop-large: 1280px;\r\n$screen-hd: 1366px;\r\n$screen-fullhd: 1440px;\r\n$screen-retina: 1660px;\r\n","css\\components\\_main-section.scss":".container {\r\n  padding-top: 50px;\r\n  margin: 0 auto;\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n}\r\n.title {\r\n  text-align: center;\r\n  text-transform: uppercase;\r\n  margin-bottom: 50px;\r\n}\r\n.list {\r\n  display: flex;\r\n  width: 100%;\r\n  flex-direction: column;\r\n  @media screen and (min-width: $screen-tablet) {\r\n    flex-direction: row;\r\n    flex-wrap: wrap;\r\n  }\r\n}\r\n.card {\r\n  transition: all ease 0.4s;\r\n  border: 3px solid $dark;\r\n  background-color: orangered;\r\n  width: 100%;\r\n  display: flex;\r\n  flex-direction: column;\r\n  &--not-done {\r\n    background-color: brown;\r\n    opacity: 0.8;\r\n    &::after {\r\n      color: white;\r\n      margin: 0 auto;\r\n      content: \"Не готово\";\r\n    }\r\n  }\r\n  &:not(:last-child) {\r\n    margin-bottom: 30px;\r\n  }\r\n  @media screen and (min-width: $screen-tablet) {\r\n    width: 48%;\r\n    flex-direction: row;\r\n    flex-wrap: wrap;\r\n    margin-left: 1%;\r\n    margin-right: 1%;\r\n    margin-bottom: 0;\r\n    margin-bottom: 30px;\r\n    &:hover {\r\n      background-color: $blue;\r\n    }\r\n  }\r\n  &__title {\r\n    text-transform: uppercase;\r\n    letter-spacing: 1px;\r\n    color: $darker;\r\n    text-align: center;\r\n    &:not(:last-child) {\r\n      margin-bottom: 20px;\r\n    }\r\n  }\r\n  &__text {\r\n    padding-left: 20px;\r\n    padding-right: 20px;\r\n    letter-spacing: 1px;\r\n    color: $dark;\r\n    text-align: justify;\r\n    &:not(:last-child) {\r\n      margin-bottom: 20px;\r\n    }\r\n  }\r\n  &__link {\r\n    padding-top: 10px;\r\n    padding-bottom: 10px;\r\n    width: 100%;\r\n    display: flex;\r\n    flex-direction: column;\r\n  }\r\n}\r\n","css\\components\\_modal.scss":".btn {\r\n  padding: 10px;\r\n}\r\n.modal {\r\n  position: fixed;\r\n  display: none;\r\n  z-index: 10;\r\n  top: 0;\r\n  left: 0;\r\n  padding-top: 200px;\r\n  width: 100%;\r\n  height: 100%;\r\n  overflow: auto;\r\n  background-color: rgb(0, 0, 0);\r\n  background-color: rgba(0, 0, 0, 0.4);\r\n\r\n  &__content {\r\n    position: relative;\r\n    background-color: #e1bee7;\r\n    margin: auto;\r\n    padding: 0;\r\n    border: 5px solid #7b1fa2;\r\n    width: 50%;\r\n    box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19);\r\n    animation-name: animatetop;\r\n    animation-duration: 1s;\r\n  }\r\n}\r\n.modal-content {\r\n  &__body {\r\n    padding: 2px 16px;\r\n  }\r\n  &__footer {\r\n    padding: 2px 16px;\r\n    background-color: $blue;\r\n    color: white;\r\n  }\r\n  &__header {\r\n    padding: 2px 16px;\r\n    background-color: red;\r\n    color: white;\r\n  }\r\n}\r\n.modal-header {\r\n  &__close {\r\n    color: white;\r\n    float: right;\r\n    font-size: 28px;\r\n    font-weight: bold;\r\n    &:hover,\r\n    &:focus {\r\n      transition: all ease 0.5s;\r\n      color: #000;\r\n      text-decoration: none;\r\n      cursor: pointer;\r\n    }\r\n  }\r\n}\r\n\r\n@keyframes animatetop {\r\n  from {\r\n    top: -300px;\r\n    opacity: 0;\r\n  }\r\n  to {\r\n    top: 0;\r\n    opacity: 1;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"f08742cb35448def6dd5af5dbdf468e4","cacheData":{"env":{}}}